{
  "name": "@uttori/data-tools",
  "version": "2.4.0",
  "description": "Tools for working with binary data.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uttori/uttori-data-tools.git"
  },
  "homepage": "https://github.com/uttori/uttori-data-tools",
  "keywords": [
    "uttori",
    "binary",
    "data",
    "parsing"
  ],
  "engines": {
    "node": ">= 14"
  },
  "optionalDependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/node": "^20.4.8",
    "@typescript-eslint/parser": "^6.2.1",
    "ava": "^5.3.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-anti-trojan-source": "^1.1.1",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-xss": "^0.1.12",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "release-it": "^16.1.3",
    "rollup": "^3.27.2",
    "sinon": "^15.2.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "esm/*",
    "src/*",
    "types/*"
  ],
  "main": "./src/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./esm/index.js"
    },
    "./data-bitstream": {
      "require": "./src/data-bitstream.js"
    },
    "./data-buffer-list": {
      "require": "./src/data-buffer-list.js"
    },
    "./data-buffer": {
      "require": "./src/data-buffer.js"
    },
    "./data-formating": {
      "require": "./src/data-formating.js"
    },
    "./data-hash-crc32": {
      "require": "./src/data-hash-crc32.js"
    },
    "./data-helpers": {
      "require": "./src/data-helpers.js"
    },
    "./data-stream": {
      "require": "./src/data-stream.js"
    },
    "./underflow-error": {
      "require": "./src/underflow-error.js"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "!src/**/*.js"
    ],
    "concurrency": 4,
    "failFast": false,
    "tap": false,
    "verbose": true
  },
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js src/data-stream.js > docs/data-stream.md; jsdoc2md --private --example-lang js src/data-hash-crc32.js > docs/data-hash-crc32.md; jsdoc2md --private --example-lang js src/data-buffer-list.js > docs/data-buffer-list.md; jsdoc2md --private --example-lang js src/data-bitstream.js > docs/data-bitstream.md; jsdoc2md --private --example-lang js src/data-buffer.js > docs/data-buffer.md; jsdoc2md --private --example-lang js src/data-helpers.js > docs/data-helpers.md; jsdoc2md --private --example-lang js src/data-formating.js > docs/data-formating.md; jsdoc2md --private --example-lang js src/encodings/shift-jis.js > docs/encodings-shift-jis.md ",
    "docs-serve": "docsify init ./docs && docsify serve ./docs",
    "lint": "eslint .",
    "make-types": "rm types/index.d.ts && tsc --project .tsconfig.json",
    "release": "release-it",
    "report": "nyc report --reporter=html",
    "test": "NODE_ENV=test nyc ava",
    "test-debug": "UTTORI_DATA_DEBUG=true DEBUG=* NODE_ENV=test ava --serial --concurrency=1",
    "validate": "npm ls"
  },
  "bugs": {
    "url": "https://github.com/uttori/uttori-data-tools/issues"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
